# Runtime: 0.115s

func count(n, k) {
    var L      = lcm(1..n)
    var period = lcm(L, n + 1)/L
    var count  = floor((k - 2) / L)
    count - floor(count / period)
}

say sum(1..31, {|k| count(k, 4**k) })
