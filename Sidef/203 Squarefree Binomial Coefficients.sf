# Runtime: 0.142s

func pascal(rows) {
    var table = Hash()
    for n in (^rows) {
        table{(0..n -> map {|k| binomial(n, k) })...} = ()
    }
    table.keys.map {|n| Num(n) }.grep { .is_square_free }.sum(0)
}

say pascal(51)
