# https://projecteuler.net/problem=173

# Runtime: 35.684s (previously: 43.581s)

var count = 0
var tiles = 1e6

for k (1 .. tiles>>2) {
    var sum = 0
    for j (k+2 .. Inf `by` 2) {
        sum += (2*j + 2*(j-2))
        (sum <= tiles) ? ++count : break
    }
}

say count
