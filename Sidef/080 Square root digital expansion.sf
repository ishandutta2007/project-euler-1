# https://projecteuler.net/problem=80

# Runtime: 0.195s

var sum = 0
local Num!PREC = 102<<2

for n in (1 .. 100) {

    next if n.is_square

    var x = Str(n.sqrt)
    x -= /\./
    x.first!(100)

    sum += Num(x).sumdigits
}

say sum
