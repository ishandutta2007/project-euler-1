# https://projecteuler.net/problem=129

# Runtime: 29.843s

for n in (1e6 .. Inf) {

    gcd(n, 10) == 1 || next

    var k = 1
    var sum = 1

    loop {
        sum += powmod(10, k, n)
        sum %= n
        sum.is_zero && break
        ++k
    }

    if (k+1 > 1e6) {
        say "A(#{n}) = #{k+1}"
        break
    }
}
